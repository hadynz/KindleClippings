{
  "name": "@hadynz/kindle-clippings",
  "version": "2.0.1",
  "description": "Package for parsing a MyClippings.txt file obtained from a Kindle and organizing it",
  "author": "Hady Osman <hadyos@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hadynz/KindleClippings"
  },
  "scripts": {
    "test": "npm run lint && jest",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts' && eslint 'src/**/*.ts'",
    "dev": "ts-node src/cli.ts",
    "prepublishOnly": "npm test",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "^27.3.1",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
